{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": [ "./src" ]
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "readme": "docs.md",
    "template": "docconfig/template",
    "encoding": "utf8",
    "destination": "./docs",
    "recurse": true
  },
  "templates": {
    "cleverLinks": false
  }
}
